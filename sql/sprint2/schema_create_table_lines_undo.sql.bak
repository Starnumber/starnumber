-- Table to create table lines, both incoming and outgoing
-- This SQL script creates the table.

DROP TABLE IF EXISTS STARNUMBER.LINES;

CREATE TABLE  STARNUMBER.LINES (
  line_id serial primary key,
  line_name varchar(80),
  user_id int REFERENCES STARNUMBER.USERS (USER_ID),
  line_type_id int REFERENCES STARNUMBER.line_types (line_type_ID)
);

COMMENT ON TABLE STARNUMBER.DESTINATION_TYPE IS 'This table tracks incoming and outgoing lines';

